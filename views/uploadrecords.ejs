        <div class="main_content">
          <h3>Upload your vinyl collection here</h3>
         <!-- <button class="main_btn"><a href="/register.ejs">Get Started</a></button>-->
         <form class="uploadrecords" method="post" action="/uploadrecords">
            <div class="collections-form">
                <input class="form-control" type="text" name="artist" placeholder="Artist">
              </div>
              <br>
            <div>
                <input class="form-control" type="text" name="album_name" placeholder="Album name">
            </div>
              <br>
            <div>
                <input class="form-control" type="text" name="genre" placeholder="Genre">
            </div>
              <br>
            <div>
                <input class="form-control" type="text" name="record_label" placeholder="Record label">
            </div>
              <br>
            <div>
                <input class="form-control" type="text" name="release_date" placeholder="Release date">
            </div>
              <br>
            <div>
                <input class="form-control" type="text" name="description" placeholder="Description">
            </div>
              <br>
            <div>
                <input class="form-control" type="text" name="image_url" placeholder="Image">
            </div>
            <br>
            <div>
                <input class="form-control" type="text" name="tracklist" placeholder="Tracklist">
            </div>
            <br>
            <div>
                <input class="form-control" type="text" name="rating" placeholder="Rating">
                <br>
                <input type ="submit" value ="Upload" class="button">
            </div>
        </form>
